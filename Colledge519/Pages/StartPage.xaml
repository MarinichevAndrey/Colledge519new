<Page x:Class="Colledge519.Pages.StartPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Colledge519.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="StartPage">

    <StackPanel Margin="0,5,0,0">
        <Label Content="Первая страница" HorizontalAlignment="Center"/>
        <Button x:Name="Back_Button" Height="25" Width="200" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="0,15,0,0" Content="Назад" Click="Back_Button_Click"/>
    </StackPanel>
</Page>
